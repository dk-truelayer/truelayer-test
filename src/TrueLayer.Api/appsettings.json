{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Features": {
    "PokemonClient": {
      "Implementation": "PokeApi",
      "PokeApi": {
        "BaseUrl": "https://pokeapi.co/api/v2"
      }
    },
    "Translation": {
      "Implementation": "FunTranslations",
      "FunTranslations": {
        "BaseUrl": "https://api.funtranslations.com/translate",
        "Endpoints": {
          "Shakespeare": "shakespeare.json",
          "Yoda": "yoda.json"
        }
      }
    }
  }
}
